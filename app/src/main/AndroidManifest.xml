<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teste.daniel.FiveFitnessThings">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dumbell"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.daniel.FiveFitnessThings.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.daniel.FiveFitnessThings.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:parentActivityName="com.daniel.FiveFitnessThings.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.daniel.FiveFitnessThings.MainActivity" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.daniel.fiveFitnessThings.Exercicio"
            android:label="@string/title_activity_exercicio"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.daniel.FiveFitnessThings.MainActivity" />
        </activity>
    </application>

</manifest>
